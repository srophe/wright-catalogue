            <msContents>
              <summary/>
              <textLang mainLang="syr"/>
              <msItem xml:id="p1a1" n="1">
                <title>A work similar in character to <ref target="http://syriaca.org/manuscript/167">the preceding</ref>, exhibiting some of the books of
                the Old Testament, in the following order</title>
                <msItem xml:id="p1b1" n="2" defective="true">
                  <locus from="1a">Fol. 1a</locus>
                  <title xml:lang="en">Ruth</title>
                </msItem>
                <msItem xml:id="p1b2" n="3" defective="true">
                  <locus from="1a">Fol. 1a</locus>
                  <title xml:lang="en">Susanna</title>
                </msItem>
                <msItem xml:id="p1b3" n="4" defective="true">
                  <locus from="1b">Fol. 1b</locus>
                  <title xml:lang="en">Esther</title>
                </msItem>
                <msItem xml:id="p1b4" n="5" defective="true">
                  <locus from="2a">Fol. 2a</locus>
                  <title xml:lang="en">Judith</title>
                </msItem>
