            <msContents>
              <textLang mainLang="syr"/>
              <msItem xml:id="p2a1" n="1">
                <title xml:lang="en">A work similar to <ref target="http://syriaca.org/manuscript/541">the preceding</ref>, but comprising not only biblical books, but also the works of several <persName>Greek Fathers</persName> and <persName>other writers</persName>
                </title>
                <note>See the passages of <persName>Bar-Hebræus</persName> quoted by <bibl>Assemani, Bibl. Or., t. iii., pars. ii., pp. 937-8</bibl>.</note>
                <note>See <ref>additions</ref>.</note>
                <msItem xml:id="p2b1" n="2">
                  <title xml:lang="en">The New Testament</title>
                  <msItem xml:id="p2c1" n="3">
                    <locus from="37a"/>
                    <title xml:lang="en">The Acts of <persName>the Apostles</persName>
                    </title>
                    <rubric xml:lang="syr">ܟܬܒܐ ܕܦܪܱܟܣܺܝܣ. ܕ܏ܝܒ. ܫܠܝ̈ܚܐ</rubric>
                    <msItem xml:id="p2d1" n="4">
                      <title xml:lang="en">The three Catholic Epistles</title>
                      <note>Appended to <ref>The Acts of the Apostles</ref>.</note>
                    </msItem>
                  </msItem>
                  <msItem xml:id="p2c2" n="5">
                    <locus from="39a"/>
                    <author>S. Paul</author>
                    <title xml:lang="en">The Epistles of <persName>S. Paul</persName>, in the usual order</title>
                  </msItem>
                  <msItem xml:id="p2c3" n="6">
                    <locus from="43a"/>
                    <title xml:lang="en">The four Gospels, according to the Peshitta version</title>
                  </msItem>
                  <msItem xml:id="p2c4" n="7">
                    <locus from="46a"/>
                    <title xml:lang="en">The four Gospels, according to the Harklensian version</title>
                    <rubric xml:lang="syr">ܡ̇ܫܠܡܢܘܬܐ ܕܬܐܘܡܐ</rubric>
                  </msItem>
                  <msItem xml:id="p2c5" n="8">
                    <locus from="48a" to="48b"/>
                    <title xml:lang="en">The genealogies of our Saviour (<ref>S. Matthew, ch. i.</ref>, and <ref>S. Luke, ch. iii.</ref>)</title>
                  </msItem>
                </msItem>
                <msItem xml:id="p2b2" n="9">
                  <title xml:lang="en">The Greek Doctors</title>
                  <msItem xml:id="p2c6" n="10">
                    <author ref="https://syriaca.org/person/452">Dionysius the Areopagite</author>
                    <title xml:lang="en">The writings of Dionysius the Areopagite, as translated
                  by <persName>Phocas bar Sergius of <placeName>Edessa</placeName>
                      </persName>
                    </title>
                    <rubric xml:lang="syr">ܬܘܒ ܟܬܒܐ ܕܩܕܝܫܐ ܕܺܝܴܢܘܣܻܝܳܘܣ ܗ̇ܘ ܕܡܢ ܐܱܪܷܐܝܳܘܣ ܦܰܐܓܴܘܣ</rubric>
                  </msItem>
                  <msItem xml:id="p2c7" n="11">
                    <locus from="51a"/>
                    <title xml:lang="en">A list of the <persName>Byzantine emperors</persName>, from <persName>Constantine the Great</persName> down to <persName>Heraclius I</persName>
                    </title>
                    <rubric xml:lang="syr">ܡ̈ܠܟܐ ܟܪ̈ܝܣܛܝܢܐ ܕܪ̈ܘܡܝܐ</rubric>
                  </msItem>
                  <msItem xml:id="p2c8" n="12">
                    <locus from="52b"/>
                    <author ref="https://syriaca.org/person/376">Basil of Caesarea</author>
                    <title xml:lang="en">The writings of Basil of <placeName>Caesarea</placeName>
                    </title>
                    <rubric xml:lang="syr">ܫܡ̈ܗܐ ܕܩܕܝܫܐ ܒܣܝܠܝܘܣ. ܐܦܝܣܩܘܦܐ ܕܗ̇ܝ ܕܩܣܪ̈ܝܐ</rubric>
                    <msItem xml:id="p2d2" n="13">
                      <locus from="66b"/>
                      <author>Joannes Maro</author>
                      <title xml:lang="en">A discourse of Joannes Maro on the Incarnation of God the Word</title>
                      <rubric xml:lang="syr">ܡܠܬܐ. ܐܝܟ ܕܒܦܣ̈ܝܩܬܐ ܕܥܠ ܡܬܒܪܢܫܢܘܬܐ ܕܡܠܬܐ ܐܠܗܐ. ܕܥܒ̣ܝܕܐ ܠܗ̇ܘ ܕܒܝܬ ܩܕܝܫ̈ܐ. ܡܪܝ ܝܘܚܢܢ. ܕܡܬܝܕܥ ܕܡܪܘܢ. ܐܝܟ ܬܪܥܝܬܐ ܕܟ̈ܠܗܘܢ ܡ̈ܠܦܢܐ ܬܪܝܨ̈ܝ ܫ܏ܘ</rubric>
                      <incipit xml:lang="syr">ܨܒ̇ܐ ܐܢܐ ܐܘ ܪܚ̇ܡ ܫܪܪܐ ܘܪܚ̇ܡ ܝܘܠܦܢܐ ܘܕܚ̇ܠ ܐܠܗܐ: ܕܠܗܠܝܢ
                    ܬܪ̈ܝܢ ܣܘܪ̈ܛܐ ܕܡܢ ܒܨܝܪܘܬܢ ܡܬܟܬܒܝܢ܆ ܕܬܗܘܐ ܕܝܢܐ ܠܐ ܢܣ̇ܒ ܒ̈ܐܦܐ܇ ܘܡܒܚܢܢܐ ܚܬܝܬܐ</incipit>
                      <note>Interposed in the <ref>writings of <persName>Basil</persName>
                        </ref>.</note>
                    </msItem>
                  </msItem>
                  <msItem xml:id="p2c9" n="14">
                    <locus from="74a"/>
                    <author ref="https://syriaca.org/person/511">Gregory Nazianzen</author>
                    <title xml:lang="en">The writings of Gregory Nazianzen, part I</title>
                    <rubric xml:lang="syr">ܫܡ̈ܗܐ ܘܩܪ̈ܝܬܐ ܕܓܪܝ݊ܓܪܝܘܣ ܐܦ܏ܝܣ ܕܰܢܙܻܝܰܢܙܾܘ</rubric>
                    <msItem xml:id="p2d3" n="15">
                      <locus from="89b"/>
                      <title xml:lang="en">A short tract on the various meanings of the word <foreign xml:lang="syr">ܡܠܬܐ</foreign>
                    in <ref>the writings of <persName>Gregory Nazianzen</persName>
                        </ref>, with examples</title>
                      <rubric xml:lang="syr">ܒܗ̈ܠܝܢ ܙܢܝ̈ܐ ܡܬܡܪܐ <foreign xml:lang="en">(sic)</foreign> ܡܠܬܐ. ܒܟܬܒܐ ܕܩܕܝܫܐ ܬܐܘ݊ܠܓܘܣ</rubric>
                      <note>Inserted into <ref>The writings of Gregory Nazianzen, part I</ref>.</note>
                    </msItem>
