                  <msItem xml:id="p1c1" n="9">
                    <locus from="23b">Fol. 23b</locus>
                    <title xml:lang="en">The Lamentations</title>
                    <note>Appended to <ref>Jeremiah</ref>.</note>
                  </msItem>
                  <msItem xml:id="p1c2" n="10">
