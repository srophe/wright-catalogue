            <textLang mainLang="syr"/>
            <msItem xml:id="a1" n="1" defective="true">
              <title xml:lang="en">Services for various Feasts, Saints' Days,
                        etc.</title>
              <note>A full service comprises <foreign xml:lang="syr">ܨܠܘܬܐ</foreign>, <foreign xml:lang="syr">ܦܪܘܡܝܘܢ,
                           ܣܕܪܐ, ܥܛܪܐ</foreign>, and <foreign xml:lang="syr">ܚܘܬܡܐ</foreign>.</note>
              <msItem xml:id="b1" n="2">
                <locus to="50" from="1a">Foll. 1a-50</locus>
                <title xml:lang="en">The Resurrection, in eight orders.</title>
                <note>The first order is wanting.</note>
                <msItem xml:id="c1" n="3" defective="true">
                  <locus to="2" from="1a">Foll. 1a-2</locus>
                  <title xml:lang="en">Second order.</title>
                  <note>It is imperfect at the beginning. </note>
                </msItem>
                <msItem xml:id="c2" n="4">
                  <locus to="10" from="2b">Foll. 2b-10</locus>
                  <title xml:lang="en">Third order.</title>
                  <rubric xml:lang="syr">ܛܟ̣ܣܐ ܬܠܝ̣ܬܝܐ ܕܩܝܡ̣ܬܐ</rubric>
                </msItem>
                <msItem xml:id="c3" n="5">
                  <locus to="16" from="10b"/>
