                  <locus to="31" from="25a">Foll. 25a-31</locus>
                  <title xml:lang="en">Sixth order.</title>
                  <note>It is Imperfect at the end.</note>
                </msItem>
                <msItem xml:id="c6" n="8" defective="true">
                  <locus to="40" from="32a">Foll. 32a-40</locus>
                  <title xml:lang="en">Seventh order.</title>
                  <note>It is imperfect at the beginning.</note>
                </msItem>
                <msItem xml:id="c7" n="9">
                  <locus to="50" from="40b">Foll. 40b-50</locus>
                  <title xml:lang="en">Eighth order.</title>
                </msItem>
              </msItem>
