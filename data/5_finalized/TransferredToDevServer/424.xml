            <textLang mainLang="syr"/>
            <msItem xml:id="a1" n="1" defective="true">
              <locus from="1a"/>
              <author ref="http://syriaca.org/person/13">Ephrem</author>
              <title xml:lang="en">A discourse in heptasyllabic metre on the <ref>Acts of <persName>S. Andrew the Apostle</persName></ref></title>
              <note>Imperfect at the beginning.</note>
              <note>Probably that ascribed to Ephraim. See <bibl>Assemani, Bibl. Or., t. i., p. 148</bibl>.</note>
              <rubric xml:lang="syr">ܐܢܕܪܐܘܣ ܫܠܝܚܐ</rubric>
            </msItem>
            <msItem xml:id="a2" n="2" defective="true">
              <locus from="4a"/>
              <author ref="http://syriaca.org/person/42">Jacob of Batnae</author>
              <title xml:lang="en">A discourse on expatriation ascribed to <persName ref="http://syriaca.org/person/42">Jacob of <placeName ref="https://syriaca.org/place/45">Batnae</placeName></persName></title>
              <note>Imperfect at the end</note>
              <rubric xml:lang="syr">ܡܐܡܪܐ ܕܡܪܝܥܩܘܒ ܕܥܠ ܐܟܣ̇ܢ</rubric>
              <incipit xml:lang="syr">ܒܚܕ ܡܢ ܝܘ̈ܡܝܢ ܒܫܘ̈ܩܝ ܥܠܡܐ ܟܕ ܪܕܐ ܗܘܝܬ. ܩ ܐܘ̈ܠܝܬܐ ܕܡ̈ܠܝܢ ܚ̈ܫܐ ܢܦܠܝ
                                ܒ̈ܐܕܢܝ ܗܦܟܬ ܘܚܙܝܬ ܠܚܕ ܡܣܟܢܐ ܘܢܘܟܪܝ ܐܬܪܐ. ܕܪܡ̣ܐ ܘܫ̣ܕܐ ܟܕ ܡܬܛ̇ܪܦ ܡܢ
                                ܟܘܪܗܢܐ </incipit>
            </msItem>
            <msItem xml:id="a3" n="3" defective="true">
              <locus from="7a"/>
              <author ref="http://syriaca.org/person/13">Ephrem</author>
              <title xml:lang="en">A funeral discourse on Strangers</title>
              <rubric xml:lang="syr">ܥܠ ܐܟܣ̈ܢܝܐ</rubric>
              <note>Very imperfect.</note>
              <note>See <bibl>Add. 14,630, fol. 13 b</bibl>.</note>
            </msItem>
            <msItem xml:id="a4" n="4">
              <locus from="7b"/>
              <author ref="https://syriaca.org/person/2816">Serapion</author>
              <title xml:lang="en">The life of <persName ref="https://syriaca.org/person/856">Macarius the Great</persName>, written by his disciple <persName ref="http://syriaca.org/person/2816">Serapion</persName></title>
              <rubric xml:lang="syr">ܬܫܥܝܬܐ ܕܩܕܝܫܐ ܐܒܐ ܡܩܪܝ ܐܒܐ ܕܕܝ̈ܪܝܐ ܕܡܕܒܪܐ ܕܡܨܪܝܢ</rubric>
              <note>See <bibl>Add. 14,732, fol. 54 b</bibl>.</note>
            </msItem>
            <msItem xml:id="a5" n="5">
              <locus from="60b"/>
              <title xml:lang="en">A prayer</title>
              <incipit xml:lang="syr">ܨܠܘܬܐ܀ ܡܪܢ ܝܫܘܥ ܡܫܝܚܐ. ܐܢܬ ܗܘ ܡܥܕܪܢܝ. ܒ̈ܐܝܕܝܟ ܐܝܬܝ. ܏ܘܫ</incipit>
            </msItem>
            <msItem xml:id="a6" n="6">
              <locus from="61a"/>
              <title xml:lang="en">The history of <persName>Martinianus</persName></title>
              <rubric xml:lang="syr">ܬܫܥܝܬܐ ܕܐܒܐ ܢܨܝܚܐ ܡܪܝ ܛܝܢܝܢܐ</rubric>
              <note>See <bibl>Add. 14,645, no. 15</bibl>.</note>
            </msItem>
            <msItem xml:id="a7" n="7" defective="true">
              <locus from="82a"/>
              <title xml:lang="en">An extract from <ref>the history of <persName ref="https://syriaca.org/person/1913">Maximus</persName> and <persName ref="https://syriaca.org/person/1374">Domitius</persName></ref></title>
              <rubric xml:lang="syr">ܦܣܘܩܐ ܡܢ ܬܫܥܝܬܐ ܕܩ̈ܕܝܫܐ ܡܟܣܝܡܘܣ ܘܕܝܡܛܝܘܣ ܐܚ̈ܐ ܒ̈ܢܝ ܡܠܟܐ ܘܠܢܛܝܘܣ ܡܗܝܡܢܐ</rubric>
              <note>See <bibl>Add. 14,732, fol. 74 b</bibl>.</note>
              <note>Imperfect at the end.</note>
            </msItem>
