            <textLang mainLang="syr"/>
            <msItem xml:id="a1" n="1" defective="true">
              <locus to="21" from="1b">Foll. 1b-21</locus>
              <title xml:lang="en">The treatise of <persName ref="http://syriaca.org/person/483">Evagrius</persName>
