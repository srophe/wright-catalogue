            <msContents>
              <textLang mainLang="syr"/>
              <msItem xml:id="p2a1" n="1">
                <locus from="48a" to="77b"/>
                <title xml:lang="en">A Small Collection of Anaphoras</title>
                <msItem xml:id="p2b1" n="2">
                  <locus from="48a" to="56"/>
                  <title xml:lang="en">Anaphora of <persName ref="http://syriaca.org/person/1871">S. Mark</persName>
                  </title>
                  <rubric xml:lang="syr">ܐܢܢܦܘܪܐ ܕܛܘܼܒܢܐ ܡܪܩܘܣ ܐܘܢܓܠܝ̣ܣܛܐ</rubric>
                  <note>See <bibl>Renaudot, Liturg. Orient., t. ii., p. 176</bibl>.</note>
                </msItem>
                <msItem xml:id="p2b2" n="3">
                  <locus from="56a" to="63"/>
                  <author ref="https://syriaca.org/person/44">Philoxenus of Mabug</author>
                  <title xml:lang="en">Anaphora of Philoxenus of Mabug</title>
                  <incipit xml:lang="syr">ܡܪܝܐ ܐܠܗܐ ܚ̇ܝ̣ܠܬܢܐ ܘܩܕܝܫܐ ܘܐܚ̣ܝܕ ܟܠ. ܏ܘܫ </incipit>
                </msItem>
                <msItem xml:id="p2b3" n="4">
                  <locus from="63b" to="77"/>
