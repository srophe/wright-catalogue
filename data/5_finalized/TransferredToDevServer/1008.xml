            <msContents>
              <textLang mainLang="syr"/>
              <msItem xml:id="p1a1" n="1" defective="true">
                <locus from="1a" to="45b"/>
                <title xml:lang="en">A Collection of Anaphoras</title>
                <msItem xml:id="p1b1" n="2" defective="true">
                  <locus from="1a" to="2"/>
                  <author ref="https://syriaca.org/person/2163">Julius, Pope of Rome</author>
                  <title xml:lang="en">Anaphora of <persName ref="https://syriaca.org/person/2163">Julius of Rome</persName>
                  </title>
                  <rubric xml:lang="syr">ܐܢܢܦܘܪܐ ܕܩܕܝܫܐ ܐܝܘܠܝܘܣ</rubric>
                  <note>Very Imperfect</note>
                </msItem>
                <msItem xml:id="p1b2" n="3" defective="true">
                  <locus from="2b" to="4"/>
                  <author ref="https://syriaca.org/person/44">Philoxenus of Mabug</author>
