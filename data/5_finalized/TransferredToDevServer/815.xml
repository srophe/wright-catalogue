                <msItem xml:id="p1b9" n="12">
                  <locus from="60a" to="64"/>
                  <rubric xml:lang="syr">ܡܛܠ ܛܘܟܣܐ ܕܐܚ̈ܐ ܫܪ̈ܘܝܐ</rubric>
