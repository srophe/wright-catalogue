              <msItem xml:id="p1a1" n="1">
                <locus from="1a" to="10"/>
                <author ref="https://syriaca.org/person/42">Jacob of Batnae</author>
                <title xml:lang="en">Metrical discourses of <persName ref="https://syriaca.org/person/42">Jacob of Batnae</persName>
