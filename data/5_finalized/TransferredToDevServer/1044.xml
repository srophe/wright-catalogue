            <msItem xml:id="a1" n="1" defective="true">
              <locus to="77" from="1">Foll. 1-77</locus>
              <title xml:lang="en">Cathismata, <foreign xml:lang="syr">ܩ̇ܐܬܝ̣̈ܣܡܐ</foreign> or <foreign xml:lang="syr">ܩܬܝ̈ܣܡܐ</foreign>
                           (<foreign xml:lang="grc">καθίσματα</foreign>), and other hymns, for
                        various festivals.</title>
