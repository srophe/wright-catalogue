                <locus to="6" from="1a">Foll. 1a-6</locus>
                <title xml:lang="en">The Dedication of the Church.</title>
                <rubric xml:lang="syr">ܕܚܘܕܬ ܥܕܬܐ</rubric>
                <msItem xml:id="c1" n="3">
                  <locus to="2" from="1a">Foll. 1a-2</locus>
                  <rubric xml:lang="syr">ܚܕܒܫܒܐ ܩܕܡܝܐ ܕܚܘܿܕܬܐ</rubric>
                </msItem>
                <msItem xml:id="c2" n="4">
                  <locus to="4" from="2b">Foll. 2b-4</locus>
                  <rubric xml:lang="syr">ܚܕܒܫܒܐ ܕܬܪ̈ܝܢ </rubric>
