            <msItem xml:id="a1" n="1" defective="true">
              <locus from="1a" to="16"/>
              <title xml:lang="en">Portions of Choral Services for the
                                festivals of the whole year, including madrashe, canons,
                                etc.</title>
              <msItem xml:id="b1" n="2">
                <locus from="1a" to="1a"/>
