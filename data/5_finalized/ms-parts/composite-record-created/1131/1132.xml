                <rubric xml:lang="syr">ܛܟ̣ܣܐ ܕܥܠ ܚܘܿܕܬ ܥܕܬܐ</rubric>
              </msItem>
              <msItem xml:id="b2" n="3">
                <locus from="1a" to="3"/>
