          <msContents>
            <textLang mainLang="syr"/>
            <msItem xml:id="a1" n="1">
              <locus from="1a"/>
              <author>Ephraim</author>
              <author>Isaac of Antioch</author>
              <author>Jacob of Batnae</author>
              <title xml:lang="en">A large collection of hymns (ܩ̈ܠܐ,ܡܕܪ̈ܫܐ andܣܘ̈ܓܝܬܐ ),
                ascribed to Ephraim, Isaac of Antioch, and Jacob of Batnae</title>
              <msItem xml:id="b1" n="2" defective="true">
                <locus from="1a"/>
                <author>Ephraim</author>
                <title xml:lang="en">Madrāshē of Ephraim for the Epiphany</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܒܝܬ ܕܢܚܐ ܕܗ̇ܘܝܢ ܒܡܢܝܢܐ ܬܠܬܥܣܪ̈</rubric>
                <note>Originally 13 in number</note>
                <note>The first five are now lost, and of the sixth only three lines remain.</note>
              </msItem>
              <msItem xml:id="b2" n="3">
                <locus from="2b"/>
                <author>Ephraim</author>
                <title xml:lang="en">Madrāshē of Ephraim for the Vigils of the
                  brethren</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܫܗܪܐ ܕܐܚ̈ܐ ܕܡܪܝ ܐܦܪܝܡ</rubric>
                <note>21 in number.</note>
                <msItem xml:id="c1" n="4">
                  <title xml:lang="en">Of the Confessors</title>
                  <rubric xml:lang="syr">ܕܥܠ ܡܘ̈ܕܝܢܐ</rubric>
                  <note>numbered ܝܚ. One from the above.</note>
                </msItem>
                <msItem xml:id="c2" n="5">
                  <author>Jacob</author>
                  <title xml:lang="en">Of the Martyrs</title>
                  <rubric xml:lang="syr">ܥܠ ܣܗ̈ܕܐ</rubric>
                  <note>numbered ܝܛ and ܟ , this item is actually two under one title</note>
                </msItem>
              </msItem>
              <msItem xml:id="b3" n="6">
                <locus from="5a"/>
                <title xml:lang="en">Penitential madrāshē</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܬܘܬ ܢܦܫܐ</rubric>
              </msItem>
              <msItem xml:id="b4" n="7">
                <locus from="5b"/>
                <title xml:lang="en">Paraenetic madrāshē</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܡܪܬܝܢܘܬܐ</rubric>
                <msItem xml:id="c3" n="8">
                  <locus from="10b"/>
                  <rubric xml:lang="syr">ܒܕܘܟܪܢܐ ܕܐܒ̈ܗܬܐ ܕܥܠ ܚܕ ܩܢܘܡܐ</rubric>
                  <note>This item has a special title among the midrashe it is with</note>
                </msItem>
                <msItem xml:id="c4" n="9">
                  <locus to="13b" from="12b"/>
                  <rubric xml:lang="syr">ܕܥܠ ܐܠܦ ܒܝܬ</rubric>
                  <note>Alphabetically arranged within the greater collection.</note>
                </msItem>
              </msItem>
              <msItem xml:id="b5" n="10">
                <locus from="19b"/>
                <author>Ephraim</author>
                <title xml:lang="en">Penitential madrāshē</title>
                <rubric xml:lang="syr">ܕܬܘܬ ܢܦܫܐ</rubric>
                <note>in part alphabetical</note>
                <note>One of them is expressly ascribed to Ephraim, fol. 22 a.</note>
              </msItem>
              <msItem xml:id="b6" n="11">
                <locus from="24a"/>
                <title xml:lang="en">On the denial of Simon Peter</title>
                <rubric xml:lang="syr">ܥܠ ܟܦܘܪܝܗ ܕܫܡܥܘܢ</rubric>
                <incipit xml:lang="syr">ܟܦ̣ܪ ܗܘܐ ܘܢܦ̣ܩ ܬܠܡܝܕܐ ܘܐܫܟܚܬܗ ܬܝܒܘܬܐ ܘܐܡ̇ܪܐ. ܠܐܝܟܐ ܥ̇ܪܩ ܐܢܬ
                  ܡܢ ܪܒܟ ܫܡܥܘܢ. ܐܝܢܐ ܐܬܪܐ ܡܩܒܠ ܠܟ. ܘܒܐܝܢܐ ܟܝ ܦܚܬܐ ܡܣܬ̣ܬܪܬܝ܀</incipit>
              </msItem>
              <msItem xml:id="b7" n="12">
                <locus from="24b"/>
                <title xml:lang="en">An alphabetical sūgīthā on <persName>Mary, the niece
                    of Abraham Kīdūnayā</persName>
                </title>
                <rubric xml:lang="syr">ܣܘܓܝܬܐ ܕܥܠ ܡܪܝܡ ܒܪܬ ܐܚܘܗܝ ܕܐܒܪܗܡ ܩܝܕܘܢܝܐ </rubric>
                <incipit xml:lang="syr">ܐ̇ܬܒ ܘܐ̇ܠܐ ܘܐ̇ܒܟܐ ܥܠ ܚ̈ܝܝ. ܘܝ ܠܝ ܕܡܢܐ ܓܕܫܢܝ. ܘܐܝܟܢܐ ܢܦ̇ܠܬ.
                  ܐܚ. ܡܢܟ ܒܝ̣ܫܐ ܡܢܐ ܥܒ̣ܕܬ ܒܝ܀ </incipit>
              </msItem>
              <msItem xml:id="b8" n="13">
                <locus from="25a"/>
                <title xml:lang="en">Another alphabetical sugitha</title>
                <rubric xml:lang="syr">ܣܘܓܝܬܐ ܐܚܪܬܐ</rubric>
                <incipit xml:lang="syr">ܐܘܼ ܕܝ̇ܢܐ ܕܕܝܢܗ ܬܪܝ̣ܨ. ܘܟܠ ܟܣ̈ܝܬܐ ܓ̈ܠܝܢ ܠܗ. ܒܕܝ̣ܢܟ ܟܐܢܐ ܠܐ
                  ܬܚܝ̇ܒܢܝ. ܡܐ ܕܟܣ̈ܝܬܐ ܡ̈ܬܓܠܝܢ܀</incipit>
                <note>Missing vowel in incipit</note>
              </msItem>
              <msItem xml:id="b9" n="14">
                <locus from="25b"/>
                <title xml:lang="en">Three madrāshē on <persName>Cyriacus, patriarch of
                    Tagrit</persName>,</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܥܠ ܩܘܪܝܩܘܣ ܦܐܛܪܝܐܪܟܐ ܕܬܓܪܝܬ </rubric>
              </msItem>
              <msItem xml:id="b10" n="15">
                <locus from="27a"/>
                <title xml:lang="en">Various madrāshē</title>
                <note>without special titles</note>
              </msItem>
              <msItem xml:id="b11" n="16">
                <locus from="37a"/>
                <author>Ephraim</author>
                <title xml:lang="en">Three madrāshē on the Church and on Virginity</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܥܠ ܥܕܬܐ ܘܕܒܬܘܠܘܬܐ</rubric>
              </msItem>
              <msItem xml:id="b12" n="17">
                <locus from="38a"/>
                <title xml:lang="en">Four madrāshē on the Oil and the Olive, and the
                  Mysteries of our Lord</title>
                <rubric xml:lang="syr">ܥܠ ܡܫܚܐ ܘܙܝܬܐ ܘܪ̈ܐܙܝ ܡܪܢ</rubric>
              </msItem>
              <msItem xml:id="b13" n="18" defective="true">
                <locus from="40a"/>
                <title xml:lang="en">Three madrāshē on the Mysteries of our Lord</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܥܠ ܪ̈ܐܙܘܗܝ ܕܡܪܢ</rubric>
                <note>The third is imperfect</note>
              </msItem>
              <msItem xml:id="b14" n="19">
                <locus from="41a"/>
                <author>Ephraim</author>
                <title xml:lang="en">Madrāshē from the collection entitled “the Hymns of
                  Nisibis,"</title>
                <note>See <bibl>Add. 14,572, and the edition of Dr. Bickell, S. Ephraemi Syri
                    Carmina Nisibena (Leipzig, 1866), p. 2.</bibl>
                </note>
                <msItem xml:id="c5" n="20">
                  <locus from="41a"/>
                  <author>Ephraim</author>
                  <title xml:lang="en">An imperfect hymn</title>
                  <note>see <bibl>Bickell, no. xv.</bibl>
                  </note>
                </msItem>
                <msItem xml:id="c6" n="21">
                  <locus from="41a"/>
                  <title xml:lang="en">Miscellaneous Piece Among the Madrashe</title>
                  <note>See<bibl> Bickell, no. xvi.</bibl>
                  </note>
                </msItem>
                <msItem xml:id="c7" n="22">
                  <locus from="41b"/>
                  <title xml:lang="en">On <persName>Abraham, bishop of
                    Nisibis</persName>
                  </title>
                  <rubric xml:lang="syr">ܕܥܠ ܐܒܪܗܡ ܐܦܝܣܩܘܦܐ ܕܢܨܝܒܝܢ</rubric>
                  <note>five hymns</note>
                  <note>See <bibl>Bickell, nos. xvii—xxi.</bibl>
                  </note>
                </msItem>
                <msItem xml:id="c8" n="23">
                  <locus from="45b"/>
                  <title xml:lang="en">On Harrān</title>
                  <rubric xml:lang="syr">ܡܕܪܫܐ ܥܠ ܚܪܢ</rubric>
                  <note>
                    <bibl>Bickell, no. xxxiv</bibl>
                  </note>
                </msItem>
                <msItem xml:id="c9" n="24" defective="true">
                  <locus from="46b"/>
                  <title xml:lang="en">On our Lord, and on Death and Satan</title>
                  <rubric xml:lang="syr">ܥܠ ܡܪܢ̣. ܘܥܠ ܡܘܬܐ ܘܣܛܢܐ</rubric>
                  <note>See <bibl>Bickell, nos. xxxv—li.</bibl>
                  </note>
                </msItem>
                <msItem xml:id="c10" n="25">
                  <locus from="58b"/>
                  <title xml:lang="en">On the Resurrection</title>
                  <rubric xml:lang="syr">ܕܢܘܚܡܐ</rubric>
                  <note>See <bibl>Bickell, nos. lxxi—lxxiii. Fol. 58 b</bibl>
                  </note>
                </msItem>
              </msItem>
              <msItem xml:id="b15" n="26">
                <locus from="66b"/>
                <author>Ephraim</author>
                <title xml:lang="en">Fifteen madrāshē on Paradise</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܚܡܫܬܥܣܪ ܕܦܪܕܝܣܐ</rubric>
                <note>See <bibl>Overbeck, S. Ephraemi Syri etc. Opera Selecta, p. xii. and p.
                    339.</bibl>
                </note>
              </msItem>
              <msItem xml:id="b16" n="27">
                <locus from="66b"/>
                <author>Ephraim</author>
                <title xml:lang="en">Madrāshē against False Doctrines</title>
                <rubric xml:lang="syr">ܕܠܘܩܒܠ ܝܘ̈ܠܦܢܐ</rubric>
              </msItem>
              <msItem xml:id="b17" n="28">
                <locus from="78b"/>
                <author>Ephraim</author>
                <title xml:lang="en">Sūgyāthā by Ephraim</title>
                <msItem xml:id="c11" n="29">
                  <locus from="78b"/>
                  <author>Ephraim</author>
                  <title xml:lang="en">Penitential</title>
                  <rubric xml:lang="syr">ܣܘܓܝܬܐ ܕܬܘܬ ܢܦܫܐ ܕܡܪܝ ܐܦܪܝܡ</rubric>
                  <incipit xml:lang="syr">ܐ̣ܘ ܬܓܪ̈ܐ ܐܙܕܗܪܘ ܒ̈ܓܙܝܟܘܢ ܡܢ ܕܝ̈ܢܐ ܕܥܠܡܐ ܡܠܐ ܬܘ̈ܟܐ.
                    ܕܟܡ̈ܐܢܘܗܝ ܩܛܝܢܝܢ ܐܢܘܢ. ܘܐܘܪ̈ܚܬܗ ܬܘܩ̈ܠܬܐ ܡ̈ܠܝܢ.</incipit>
                </msItem>
                <msItem xml:id="c12" n="30">
                  <locus from="79b"/>
                  <author>Ephraim</author>
                  <title xml:lang="en">On Joseph</title>
                  <rubric xml:lang="syr">ܣܘܓܝܬܐ ܕܥܠ ܝܘܣܦ</rubric>
                  <incipit xml:lang="syr">: ܐܡܪܐ ܠܗ ܒܬܘܠܬܐ ܠܝܘܣܦ ܗܒܝܗܝ ܠܝ ܠܗ̇ܘ ܡܪܝ ܘܪܚܘܡܝ ܐܝܢܩܝܘܗܝ
                    ܘܐ̇ܣܝܡܝܘܗܝ ܕܒܓܘ ܥܘܒܝ ܠܥܝܪܐ ܕܠܐ ܕܡ̇ܟ. ܕܥܝܪ̈ܝ ܪܘܡܐ ܒܬܫܡܫܬܗ ܪܡܝܢ܀</incipit>
                </msItem>
                <msItem xml:id="c13" n="31" defective="false">
                  <locus from="80a"/>
                  <title xml:lang="en">On the Virgin Mary</title>
                  <rubric xml:lang="syr">ܐܚܪܬܐ ܥܠ ܝܠܕܬ ܐܠܗܐ ܡܪܝܡ</rubric>
                  <incipit xml:lang="syr">ܐܡ̣ܪ ܠܝ ܡܪܝܡ ܐܝܟܢ ܐܫܟ̣ܚܬ ܘܛܥܢܬܝܘܗܝ ܠܐܠܗܐ ܡܠܬܐ. ܟܕ ܐܠܗܐ ܗܘ
                    ܨܝܕ ܐܒܘܗܝ. ܘܠܝ ܛܘܒܝ ܠܝ ܕܠܐܠܗܐ ܝ̇ܠܕܬ܀ </incipit>
                </msItem>
                <msItem xml:id="c14" n="32">
                  <locus from="80a"/>
                  <title xml:lang="en">On the holy Church</title>
                  <rubric xml:lang="syr">ܐܚܪܬܐ ܕܥܠ ܥܕܬܐ ܩܕܝܫܬܐ</rubric>
                  <incipit xml:lang="syr">ܛܠܝܬܐ ܕܢܚܬܐ ܠܓ̈ܢܐ ܘܫܦܝܪ ܠܗ̇ ܕܢܚܬܐ ܠܓ̈ܢܐ. ܕܪܚܡܬܗ ܠܓܢܢܐ ܏ܗ.
                    ܘܝܗܒ ܠܗ̇ ܩ̈ܠܝܕܐ ܕܗ̇ܝ ܓܢܬܐ ܐܘ ܛܠܝܬܐ ܟܡܐ ܫܦܝܪܬܝ. ܏ܗܠ܀</incipit>
                </msItem>
                <msItem xml:id="c15" n="33">
                  <locus from="80b"/>
                  <title xml:lang="en">On the Months of the Year</title>
                  <rubric xml:lang="syr">ܣܘܓܝܬܐ ܕܥܠ ܝܪ̈ܚܐ ܕܫܢܬܐ</rubric>
                  <incipit xml:lang="syr">ܥܠ ܝܪ̈ܚܐ ܘܥܠ ܝܘܡ̈ܬܐ ܘܥܠ ܫܢܬܐ ܕܒܥ̇ܐ ܐܢܐ ܐ̇ܣܝܡ ܡܐܡܪܐ ܐ̇ܩܘܡ
                    ܥܠ ܚܘܫܒ̈ܢܝܗܘܢ. ܘܐ̇ܒܕܩ ܫܡ̈ܘܬܐ ܕܟܠܗܘܢ܀</incipit>
                </msItem>
              </msItem>
              <msItem xml:id="b18" n="34">
                <locus from="81a"/>
                <author>Ephraim</author>
                <title xml:lang="en">Madrāshē of Ephraim, to be sung at table on the Vigils
                  of the Saints and the Dead</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܡܪܝ ܐܦܪܝܡ ܡܠܦܢܐ ܕܡܬܐܡܪ̈ܝܢ ܥܠ ܦܬܘܪܐ ܒܫܗܪܐ ܕܩ̈ܕܝܫܐ
                  ܘܕܥ̈ܢܝܕܐ </rubric>
              </msItem>
              <msItem xml:id="b19" n="35">
                <locus from="83b"/>
                <title xml:lang="en">Madrāshā on Simon Peter</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܥܠ ܫܡܥܘܢ ܟܐܦܐ</rubric>
                <incipit xml:lang="syr">ܛܘܒܝܟ ܫܡܥܘܢ ܪܝܫ ܬܠܡ̈ܝܕܐ ܕܒܪ ܐܠܗܐ ܛܘܒܐ ܝܗܒ ܠܟ.</incipit>
              </msItem>
              <msItem xml:id="b20" n="36">
                <locus from="84a"/>
                <title xml:lang="en">Madrāshā on S. Paul</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܥܠ ܦܘܠܘܣ</rubric>
                <incipit xml:lang="syr">ܛܘܒܝܟ ܦܘܠܘܣ ܒܪ ܥܒܪ̈ܝܐ ܡܐܢܐ ܓܒܝܐ ܡܲܠܐ ܢܨܚ̈ܢܐ.</incipit>
              </msItem>
              <msItem xml:id="b21" n="37">
                <locus from="85a"/>
                <title xml:lang="en">Madrāshā on S. Thomas</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܥܠ ܬܐܘܡܐ ܫܠܝܚܐ</rubric>
                <incipit xml:lang="syr">ܛܘܒܝܟ ܬܐܘܡܐ ܪܚܝܡ ܬܠܡ̈ܝܕܐ ܓ݂ܙܐ ܕܫܘ̈ܦܪܐ ܘܡܲܠܐ ܛܘ̈ܒܐ.</incipit>
              </msItem>
              <msItem xml:id="b22" n="38">
                <locus from="86a"/>
                <title xml:lang="en">Madrāshā on Judas the Traitor</title>
                <rubric xml:lang="syr">ܡܕܪ̈ܫܐ ܕܥܠ ܝܗܘܕܐ ܡܫܠܡܢܐ</rubric>
                <incipit xml:lang="syr">ܘܝ ܠܟ ܝܗܘܕܐ ܡܫܠܡ ܡܪܗ. ܓܒܝܐ ܕܢܦ̣ܠ ܡܢ ܓܒܝܘܬܗ.</incipit>
              </msItem>
              <msItem xml:id="b23" n="39">
                <locus from="86a"/>
                <title xml:lang="en">Other madrāshē</title>
                <note>Without special Titles</note>
              </msItem>
              <msItem xml:id="b24" n="40">
                <locus from="96a"/>
                <title xml:lang="en">A madrāshā to be used at the Consecration of a
                  Bishop</title>
                <rubric xml:lang="syr">ܐܚܪܢܐ ܕܩܘܒܠܐ ܕܐ̈ܦܝ܏ܣܩ</rubric>
                <incipit xml:lang="syr">ܫܠܡ ܠܟ ܪܥܝܐ ܫܪܝܪܐ. ܥܢܟ ܨ̣ܗܝܐ ܠܚܙܬܟ. ܘܡܣܟܝܐ ܠܟ
                  ܡܪܥܝܬܟ.</incipit>
              </msItem>
              <msItem xml:id="b25" n="41">
                <locus from="96a"/>
                <title xml:lang="en">Madrāshē for the Dead</title>
                <rubric xml:lang="syr">ܕܥ̈ܢܝܕܐ</rubric>
                <incipit xml:lang="syr">ܫܠ̣ܡܘ ܡܕܪ̈ܫܐ ܕܩܕܝܫܐ ܡܪܝ ܐܦܪܝܡ ܡܠܦܢܐ̇ ܕܝܠܕܬ ܐܠܗܐ ܘܕܩ̈ܕܝܫܐ
                  ܘܕܥܢܝ̈ܕܐ.</incipit>
              </msItem>
              <msItem xml:id="b26" n="42">
                <title xml:lang="en">A collection of Sūgyāthā</title>
                <rubric xml:lang="syr">ܛܟܣܐ ܕܣܘ̈ܓܝܬܐ ܕܟܠ ܓܢܣ ܘܕܟܠܝܘܡ.</rubric>
                <msItem xml:id="c16" n="43">
                  <locus from="98b"/>
                  <title xml:lang="en">On Penitence</title>
                  <rubric xml:lang="syr">ܕܥܠ ܬܘܬ ܢܦܫܐ</rubric>
                  <incipit xml:lang="syr">ܐܘ ܢܦܫܐ ܨܘܪܬܐ ܕܢܗܝܪܐ ܡ̇ܢ ܫ̇ܠܚ ܫܘܦܪ̈ܝܟܝ ܘܐܗܠ ܒܟܝ. ܘܗܐ ܣܢܝܬܝ
                    ܒܣܘܓܐܐ ܕܒܝ̈ܫܬܟܝ܀</incipit>
                </msItem>
                <msItem xml:id="c17" n="44">
                  <locus from="98b"/>
                  <title xml:lang="en">Alphabetical (Arranged Madrasha)</title>
                  <rubric xml:lang="syr">ܣܘܓܝܬܐ ܐܚܪܬܐ ܥܠ ܐܠܦ ܒܝܬ</rubric>
                  <incipit xml:lang="syr">ܐܫܬܠܚ ܘܢܚܬ ܐܡܪܐ ܒܪܝܟܐ ܠܐܦܪܬܐ ܐܡ̇ܪܐ ܥܕܬܐ ܕܐܩܘܡ ܐܙܠ ܠܝ ܒܬܪܗ
                    ܘܐܫܒܚ ܠܫܡܗ ܕܡܪܝܐ.</incipit>
                </msItem>
                <msItem xml:id="c18" n="45">
                  <locus from="99a"/>
                  <title xml:lang="en">Alphabetical (Arranged Madrasha)</title>
                  <rubric xml:lang="syr">ܐܚܪܬܐ ܕܟܠܝܘܡ</rubric>
                  <incipit xml:lang="syr">ܐܠܦ ܐܡ̇ܪܐ. ܐܘܕܘ ܠܡܪܝܡܐ ܕܝܗ̇ܒ ܠܟܘܢ ܣܡ ܚ̈ܝܐ
                    ܒܡܐܬܝܗ܀</incipit>
                </msItem>
                <msItem xml:id="c19" n="46">
                  <title xml:lang="en">On the World</title>
                  <rubric xml:lang="syr">ܕܥܠ ܥܠܡܐ</rubric>
                  <incipit xml:lang="syr">ܐܠܦ ܐܝܟܢ ܐܚܘܪ ܒܟ ܥܠܡܐ ܕܡ̇ܠܐ ܥܩ̈ܬܐ. ܕܒܟܠܗܝܢ ܢܟܠܐ ܡܠܐ
                    ܚܘܒܟ܀</incipit>
                  <quote xml:lang="syr">ܟܦ. ܠܡܕ. ܐܠܦ. ܒܝܬ. ܓܡܠ. ܕܠܕ. ܗܐ. ܘܐܘ. ܙܐܝ. ܚܝܬ. ܛܝܬ. ܝܘܕ.
                    ܟܐܦ , ܬܘܡܝܡ. ܢܘܢ ܣܡܟܬ. ܥܐܐ. ܦܐܐ , ܦܐ. ܨܐܕܐ. ܩܘܦ. ܪܝܫ. ܫܝܢ. ܬܐܘ , 
                  </quote>
                  <note>Regarding the quote: @ In these three alphabetical #sugyatha, the names of
                    the letters are written as follows</note>
                </msItem>
                <msItem xml:id="c20" n="47">
                  <locus from="99b"/>
                  <title xml:lang="en">On the Months of the Year, disputing with one
                    another</title>
                  <incipit xml:lang="syr">ܕܥܠ ܝܪ̈ܚܝ ܫܢܬܐ: ܟܕ ܕܪ̈ܫܝܢ ܚܕ ܠܘܩܒܠ ܚܕ ܗܟܢܐ: ܝܪ̈ܚܝ ܫܢܬܐ
                    ܡ̈ܬܟܢܫܝܢ. ܕܢܬܠܘܢ ܫܘܦܪܐ ܕܥ̈ܠܠܬܗܘܢ (sic) ܝ̇ܬܒܐ ܫܢܬܐ ܐܝܟ ܡܪܬܐ. ܕܬܫܡ̣ܥ ܕܝܢ̣ܐ
                    ܕܒܝ̈ܢܬܗܘܢ. ܬܘ ܫܡܥܘ ܝܪ̈ܚܐ ܡܘܢ ܐܡܪܝܢ ܘܗܒܘ ܬܫܒܘܚܬܐ ܠܒܪܘܝܗܘܢ܀</incipit>
                </msItem>
                <msItem xml:id="c21" n="48">
                  <locus from="100a"/>
                  <title xml:lang="en">On the virgin Mary, complaining of the World</title>
                  <rubric xml:lang="syr">ܕܥܠ ܒܬܘܠܬܐ ܡܪܝܡ. ܟܕ ܩ̇ܒܠܐ ܥܠ ܥܠܡܐ</rubric>
                  <incipit xml:lang="syr">ܐܢܐ ܒܩܢܘܡܝ ܚܪܬ. ܘܐܬܕܡܪܬ ܓܝܪ ܒܐܠܗܐ. ܕܨ̣ܒܐ ܒܝ ܘܥܒܕܢܝ ܐܡܗ.
                    ܘܛ̇ܠܡ ܠܝ ܥܡܐ ܕܓ̇ܪܬ. ܕܝ̇ܢܐ ܕܛܠܝܡ̈ܐ ܩܘܡ ܠܝ܀ </incipit>
                </msItem>
                <msItem xml:id="c22" n="49">
                  <locus from="100b"/>
                  <author>Isaac</author>
                  <title xml:lang="en">On the blessed Virgin and on sceptics</title>
                  <rubric xml:lang="syr">ܕܥܠ ܛܘܒܢܝܬܐ ܘܥܠ ܒܨ̈ܘܝܐ ܕܡܪܝ ܐܝܣܚܩ</rubric>
                  <incipit xml:lang="syr">ܥܡ̈ܡܐ ܬܘ ܨܘܬܘ ܫܡ̣ܥܘ. ܠܡܪܝܡ ܟܕ ܩ̇ܒܠܐ ܥܠ ܐܝܠܝܢ ܕܐܡܪܝܢ ܕܠܘ
                    ܐܠܗܐ ܗܘ ܒܪܗ.܀</incipit>
                </msItem>
                <msItem xml:id="c23" n="50">
                  <locus from="100b"/>
                  <author>Jacob</author>
                  <title xml:lang="en">On Joseph and his mistress</title>
                  <rubric xml:lang="syr">ܕܝܘܣܦ ܟܐܢܐ ܘܥܠ ܡܪܬܗ ܕܡܪܝ ܝܥܩܘܒ ܡܠܦܢܐ</rubric>
                  <incipit xml:lang="syr">ܫܪܒܗ ܕܝܘܣܦ ܒܪܥܝܢܝ ܦܠ̣ܓ. ܟܕ ܒܕܪܫܐ ܪܡ݅ܐ ܪܒ݅ܐ. ܘܒܬܟܬܘܫܐ
                    ܕܡܨܪܝܬܐ. ܩܐ̇ܡ ܟܐܢܐ ܘܠܐ ܡܬܬܙܝܥ܀</incipit>
                </msItem>
                <msItem xml:id="c24" n="51">
                  <author>Jacob</author>
                  <title xml:lang="en">On the female Sinner</title>
                  <rubric xml:lang="syr">ܕܥܠ ܗ̇ܝ ܚܛܝܬܐ ܕܡܪܝ ܝܥܩܘܒ</rubric>
                  <incipit xml:lang="syr">ܛܒܗ ܫܡܲܥܬ ܚ̇ܛܝܬܐ. ܕܒܫܪܘܬܐ ܕܫܡܥܘܢ ܓܢܐ. ܒܠܒܗ̇ ܐܡ̣ܪܬ݁ ܐܢܐ
                    ܐܙܠܢܝ. ܟܒܪ ܫܒ̇ܩ ܠܝ ܟܠ ܕܐܣܟܠܬܝ܀</incipit>
                </msItem>
                <msItem xml:id="c25" n="52">
                  <locus from="101b"/>
                  <author>Jacob</author>
                  <title xml:lang="en">On the Synagogue and the Church</title>
                  <rubric xml:lang="syr">ܕܡܪܝ ܝܥܩܘܒ ܕܥܠ ܟܢܘܫܬܐ ܘܥܕܬܐ</rubric>
                  <incipit xml:lang="syr">ܗܐ ܠܥ̈ܕܬܐ ܕܪܫܐ ܢܦܠ. ܘܒܬܟܬܘܫܐ ܗܐ ܩܝܡ̈ܢ. ܢܫܡܥ ܐ̈ܚܝ ܡܘܢ ܐܡܪ̈ܢ
                    ܩ̣ܕܡ ܟܐܢܘܬܗ ܕܗ̣ܝ ܕܝܢ̣ܐ. ܏ܘܫ.</incipit>
                </msItem>
                <msItem xml:id="c26" n="53">
                  <locus from="102b"/>
                  <author>Isaac</author>
                  <title xml:lang="en">On the World</title>
                  <rubric xml:lang="syr">ܕܡܪܝ ܐܝܣܚܩ ܕܥܠ ܥܠܡܐ</rubric>
                  <incipit xml:lang="syr">ܘܝ ܐ̈ܚܝ ܡܐ ܢܟܝܠ. ܥܠ ܪ̈ܚܡܘܗܝ ܥܠܡܐ. ܕܚ̈ܕܘܬܗ ܥܒܪ̈ܢ ܘܢܝ̈ܚܘܗܝ
                    ܦܛܪ̈ܝܢ.܀</incipit>
                </msItem>
                <msItem xml:id="c27" n="54">
                  <locus from="103a"/>
                  <title xml:lang="en">On our Lord's appearance to Cleopas and his brother,
                    after his resurrection</title>
                  <rubric xml:lang="syr">ܕܡܪܝ ܐܝܣܚܩ ܥܠ ܩܝܡܬܗ ܕܡܪܢ ܟܕ ܐܬ݁ܚܙܝ ܠܩܠܝܘܦܐ ܘܠܐܚܘܗܝ</rubric>
                  <incipit xml:lang="syr">ܫܡܫܐ ܡܢ ܓܘ ܫܝܘܠ ܕܢܚ̣ ܒܐܪܥܐ ܘܐܢܗܪܗ. ܘܗܐ ܒܕܢܚܗ ܪ̈ܘܙܢ ܒܪ̈ܝܬܐ
                    ܟܠܗ̈ܝܢ܀</incipit>
                </msItem>
                <msItem xml:id="c28" n="55">
                  <locus from="103b"/>
                  <author>Isaac</author>
                  <title xml:lang="en">On our Lord and the Woman of Samaria</title>
                  <rubric xml:lang="syr">ܕܡܪܝ ܐܝܣܚܩ ܥܠ ܡܪܢ ܘܥܠ ܫܡܪܝܬܐ</rubric>
                  <incipit xml:lang="syr">ܡܪܢ ܐܚ̈ܝ ܣܡܟ. ܥܠ ܒܐܪܥܐ ܕܫܡܪܝܢ. ܘܟܕ ܠܐܝܐ ܠܝ ܕܥܘܠܗ ܕܟܪܟܐ
                    ܢܣܝܦ.܀</incipit>
                </msItem>
                <msItem xml:id="c29" n="56">
                  <locus from="104b"/>
                  <author>Isaac</author>
                  <title xml:lang="en">On Jephthah and bis daughter</title>
                  <incipit xml:lang="syr">ܒܢ̈ܝܢܫܐ ܕܢܕܪܝܢ ܢܕܪ̈ܐ ܒܐܘܠܨܢܗܘܢ. ܙܕܩ̇ ܗܘ ܕܢܦܪܥܘܢ ܠܡܪܝܐ
                    ܒܐܪܘܚܬܗܘܢ. ܐܢ ܢܕܪܐ ܢܕܪܬ ܛܝܒ ܡܘܢ ܝܗܒܬ. ܕܠܐ ܢܕܪܟ ܢܣܬܠܐ. ܘܩܘܪܒܢܟ ܢܫܬܕܐ܀</incipit>
                </msItem>
                <msItem xml:id="c30" n="57">
                  <locus from="105a"/>
                  <title xml:lang="en">On Joseph and his mistress</title>
                  <rubric xml:lang="syr">ܕܝܘܣܦ ܟܐܢܐ ܘܥܠ ܡܪܬܗ</rubric>
                  <incipit xml:lang="syr">ܐܚ̈ܝ ܫܡܥܘ. ܣܘܓܝܬܗ. ܕܝܘܣܦ ܟܐܢܐ ܘܕܡܪܬܗ. ܘܛܝܒ ܪܥܝܢܟܘܢ. ܠܡܕܡ
                    ܕܐܡ̇ܪ ܐܢܐ ܠܟܘܢ. ܗܠܠܘܝܐ..܀</incipit>
                </msItem>
                <msItem xml:id="c31" n="58">
                  <locus from="105a"/>
                  <author>Jacob</author>
                  <title xml:lang="en">On the blessed Mary nursing</title>
                  <rubric xml:lang="syr">ܕܥܠ ܛܘܒܢܝܬܐ ܡܪܝܡ ܟܕ ܡܝܢܩܐ ܕܡܪܝܥܩܘܒ</rubric>
                  <incipit xml:lang="syr">ܪܝܡ ܬܗ̣ܪܬ݁ ܟܕ ܡܝܢܩܐ. ܠܙܐ̇ܢ ܥܠܡ̈ܐ ܕܥܘܼܠܐ ܗܘ̣ܐ܀</incipit>
                </msItem>
                <msItem xml:id="c32" n="59">
                  <locus from="105b"/>
                  <author>Jacob</author>
                  <title xml:lang="en">On Eve and Mary</title>
                  <rubric xml:lang="syr">ܕܡܪܝ ܝܥܩܘܒ ܡܠܦܢܐ̇ ܕܥܠ ܚܘܐ ܘܡܪܝܡ </rubric>
                  <incipit xml:lang="syr">ܚܘܐ ܒܥܕܝܢ ܟܬܒ̣ܬ݁. ܗ̇ܘ ܐܫܛܪܐ ܪܒܐ ܕܚܘܒܬܐ. ܕܢܬܬܒܥܘܢ ܝܠܕ̈ܝܗ̇
                    ܡܘܬܐ ܠܕܪ̈ܝܗܘܢ. ܚܘܝܐ ܣܦܪܐ ܗܦܝܟܐ̣. ܟܬܒ̣ ܚܬܡ. ܘܒܢܟܠܗ̇ ܛܒܥܗ̇.܀</incipit>
                </msItem>
                <msItem xml:id="c33" n="60">
                  <locus from="107a"/>
                  <author>Jacob</author>
                  <title xml:lang="en">On those who disputed against Mary</title>
                  <rubric xml:lang="syr">ܕܥܠ ܕܪ̈ܘܫܐ ܕܠܘܩܒܠ ܡܪܝܡ: ܕܡܪܝܥܩܘܒ</rubric>
                  <incipit xml:lang="syr">ܒܬܘܠܬܐ ܒܪ̣ܬ݁ ܕܘܝܕ ܗܐ ܩ̇ܝܡܐ ܗ̣ܘܬ݁ ܒܝܬ ܟܢ̈ܫܐ. ܘܛܥܝܢܐ ܛܠܝܐ.
                    ܘܟܪܝܟܝܢ ܠܗ̇ ܕܪ̈ܘܫܐ. ܗ̇ܘ ܕܪܫ̇ ܠܗ̇ ܗ̇ܘ ܒܨ̇ܐ ܠܗ̇ ܐܚܪܢܐ ܒ̇ܪܟ ܘܣ̇ܓܕ ܠܗ̇.܀</incipit>
                </msItem>
                <msItem xml:id="c34" n="61">
                  <locus from="107b"/>
                  <author>Jacob</author>
                  <title xml:lang="en">Paraenetic</title>
                  <rubric xml:lang="syr">ܣܘܓܝܬܐ ܕܡܪܝ ܝܥܩܘܒ ܐܚܪܬܐ ܕܡܪܬܝܢܘܬܐ</rubric>
                  <incipit xml:lang="syr">ܒܪܗܛܐ ܪܒܐ ܕܒܪ̈ܝܬܐ. ܟܕ ܚ̣ܪܬ݁ ܕܘܡܪܐ ܐܬܡܠܝܬ. ܕܚܙܝܬ ܫܘܚܠܦܐ
                    ܕܙܒ̈ܢܐ ܕܡܬܟܪ̈ܟܝܢ. ܘܕܠܐ ܫܠܝܐ ܒܚܕ ܚܐܦܐ. ܡܘܙܠܬܐ ܪ̈ܟܝܒܝܢ ܘܪ̈ܗܛܝܢ.܀</incipit>
                </msItem>
                <msItem xml:id="c35" n="62">
                  <locus from="108b"/>
                  <author>Jacob</author>
                  <title xml:lang="en">On Job and his wife</title>
                  <rubric xml:lang="syr">ܕܡܪܝ ܝܥܩܘܒ ܕܥܠ ܐܝܘܒ ܘܐܢܬܬܗ</rubric>
                  <incipit xml:lang="syr">ܐܚ̈ܝ. ܒ̣ܝܫܐ ܩܪܒܐ ܥܒ̣ܕ. ܥܡ ܙܕܝܩܐ ܟܕ ܢܣܝܗ. ܐܫܬܗ̣ܝ ܠܗ ܕܟܡܐ
                    ܡܚ̣ܝܗܝ. ܘܠܐ ܗܘ ܪܛ̣ܢ ܕܟܡܐ ܒܠ̣ܥ.܀ </incipit>
                </msItem>
                <msItem xml:id="c36" n="63">
                  <locus from="108b"/>
                  <title xml:lang="en">On the Cither</title>
                  <rubric xml:lang="syr">ܕܡܪܝ ܐܦܪܝܡ ܕܥܠ ܩܝܬܪܐ</rubric>
                  <incipit xml:lang="syr">ܐܒܘܒܐ ܪܘܚܢܐ. ܩܝܬܪܐ ܩܘܫ ܒܙܡ̇ܪܐ. ܘܚ̇ܕܐ ܚܕܘ̈ܓܐ ܕܟܠܬܐ. ܒܚܠܘܠܗ
                    ܕܡܪܝܐ ܡܪܟ. ܗ̇ܠܠ ܒܠܠܝܐ ܘܐܘܕܐ ܒܐܝܡܡܐ.܀</incipit>
                </msItem>
                <msItem xml:id="c37" n="64">
                  <locus from="109a"/>
                  <title xml:lang="en">On Mary and Joseph</title>
                  <rubric xml:lang="syr">ܕܥܠ ܡܪܝܡ ܝܠܕܬ ܐܠܗܐ ܘܝܘܣܦ</rubric>
                  <incipit xml:lang="syr">ܐܡܪܐ ܠܗ ܒܬܘܠܬܐ ܠܝܘܣܦ ܗܒܝܗܝ ܏ܘܫ</incipit>
                  <note>See above, no. 17, b.</note>
                  <note>This is in a different handwriting from the rest. On foll. 109 b and 110 a
                    there is written by a later hand </note>
                </msItem>
                <msItem xml:id="c38" n="65">
                  <title xml:lang="en">A sūgīthā on the Great Church of Edessa, built by
                      <persName>king Abgar</persName> and <bibl>Addai the Apostle</bibl>
                  </title>
                  <rubric xml:lang="syr">( ܣܘܓܝܬܐ ܥܠ ܥܕܬܐ ܪܒܬܐ ܕܐܘܪܗܝ )ܥܠ ܗܝܟܠܐ ܕܒܢܐ ܐܒܓܪ ܘܐܕܝ
                    ܫܠܝܚܐ</rubric>
                  <incipit xml:lang="syr">.ܗܒ ܠܝ ܛܝܒܘܬܐ ܕܪܘܚ ܩܘܕܫܐ. ܕܥܠ ܗܝܟܠܐ ܕܒܐܘܪܗܝ ܐܡܠܠ.
                    (suprascript: ) ܐܝܬܝܐ ܕܫܸܪܐ ܒܗܝܟܠ ܩܘܕܫܐ. ܕܫܘܒܚܗ ܡܢܗ ܗܘ ܡܬܘܡܝܬ </incipit>
                  <note>The portion of the rubric enclosed by parentheses is written upon the
                    margin</note>
                </msItem>
              </msItem>
            </msItem>
          </msContents>
