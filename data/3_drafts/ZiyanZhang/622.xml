                <msItem>
                  <locus to="" from="15a"/>
                  <author ref="http://syriaca.org/person/2703">S. Matthew</author>
                  <title ref="" xml:lang="en">On <persName ref="http://syriaca.org/person/2703">S. Matthew</persName>, ch. x. 34</title>
