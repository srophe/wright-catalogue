                  <locus from="32a">Fol.32a</locus>
                  <title xml:lang="en">Chap. xx. Add. 17,210</title>
                </msItem>
                <msItem n="21" xml:id="p4b20">
                  <locus from="52a">Fol.52a</locus>
                  <title xml:lang="en">Chap. xxi. Add. 17,211</title>
