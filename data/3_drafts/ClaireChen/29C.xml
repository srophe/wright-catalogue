              <msItem>
                <locus to="" from="132b"/>
                <author ref="">Jacob of Batnae</author>
                <title ref="" xml:lang="en">A series of extracts from Jacob of Batnae</title>
                <rubric xml:lang="syr"/>
                <incipit xml:lang="syr"/>
                <quote xml:lang="syr">
                  <locus from="" to=""/>
                </quote>
